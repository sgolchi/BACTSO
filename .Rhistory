ss = function(par){
P_a = par[1]
P_b = par[2]
p = par[3]
P_ab = logistic(max(logit(P_a), logit(P_b)) + p * min(logit(P_a), logit(P_b)))
n1 = floor(((P_a * (1 - P_a) + P_ab * (1 - P_ab)) / (P_a - P_ab)^2)* 7.9)
n2 = floor(((P_b * (1 - P_b) + P_ab * (1 - P_ab)) / (P_b - P_ab)^2)* 7.9)
return(max(n1, 2))
}
